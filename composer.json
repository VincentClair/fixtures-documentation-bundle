{
  "name": "arnaud-large/fixtures-doc",
  "description": "Fixtures documentation generator",
  "keywords": ["fixture", "data", "test"],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Arnaud Large"
    },
    {
      "name": "Alain Begey"
    }
  ],

  "require": {
    "php": "^7.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "symfony/phpunit-bridge": "^3.4.5 || ^4.0.5"
  },
  "autoload": {
    "psr-4": {
      "FixturesDoc\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FixturesDoc\\": [
        "tests"
      ]
    }
  }
}
